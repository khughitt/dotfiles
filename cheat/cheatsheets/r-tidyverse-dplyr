# summarize_all
dat %>%
  group_by(symbol) %>%
  summarize(across(everything(), max))

# select by string variable
dat %>%
    select(!!var_name)

# arrange by string variable
dat %>%
    arrange(!!sym(var_name))

# rbind multiple tibbles
bind_rows(df1, df2, ..)

# drop duplicates
dat %>%
  distinct()
