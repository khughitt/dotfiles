# angle brackets
\langle
\rangle

# arrows
\leftarrow
\longleftarrow
\Leftarrow

# double arrow
\leftrightarrow

# parens
\left( 
\right)

# equivalence
\equiv

# inequalities
\approx
\sim
\propto
\neq

# multiplication / times
\times

# tensor product
\otimes

# integration
\int_{from}^{to} 

# limits
\[ \lim_{x\to\infty} f(x) \]

# grad
\nabla

# partial derivative
\frac { \partial f } { \partial x }

# sets/logic
\forall

# sets (∈/∉)
\in
\notin

# sets
\cup      # union
\bigcup
\cap      # intersection
\bigcap

# set differences ("\")
\setminus

# proper subset (⊂)
\subset

# subset (⊆)
\subseteq

# empty set
\emptyset
\varnothing   # amssymb

# infinity
\infty

# up tack / falsum / independence of random variables (⊥)
\bot

# perpendicular (⊥)
\perp

# function compose
\circ

# text formatting
\textbf{}
\textit{}
\underline{}

# ℝ (real numbers)
\mathbb{R}

# stylized letters
# https://en.wikibooks.org/wiki/LaTeX/Mathematics#Formatting_mathematics_symbols
\mathcal{A}   # calligraphic / fancy
\mathfrak{B}  # calligraphic / fraktur
\mathbb{C}    # ℝ style
\mathbf{x}    # vectors

# dot above
\dot{x}
\ddot{x}

# hat above
\hat{}
\bar{}
\tilde{}

# vectors
\vec{}

# matrices;
# bmatrix (brackets), pmatrix (parens), matrix (no border), Bmatrix (curly braces), vmatrix (pipes)
\begin{bmatrix}
1 & 2 & 3\\
a & b & c
\end{bmatrix}

# spacing
\                # single space
\\               # line break / newline
\hspace{1cm}     # 1cm space 
\break
