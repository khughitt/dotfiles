# angle brackets
\langle
\rangle

# arrows
\leftarrow
\longleftarrow
\Leftarrow

# double arrow
\leftrightarrow

# inequalities
\approx
\sim
\propto
\neq

# multiplication / times
\times

# integration
\int_{from}^{to} 

# limits
\[ \lim_{x\to\infty} f(x) \]

# grad
\nabla

# partial derivative
\frac { \partial f } { \partial x }

# sets (∈/∉)
\in
\notin

# sets
\cup      # union
\bigcup
\cap      # intersection
\bigcap

# set differences ("\")
\setminus

# proper subset (⊂)
\subset

# subset (⊆)
\subseteq

# empty set
\emptyset
\varnothing   # amssymb

# infinity
\infty

# text formatting
\textbf{}
\textit{}
\underline{}

# ℝ (real numbers)
\mathbb{R}

# stylized letters
# https://en.wikibooks.org/wiki/LaTeX/Mathematics#Formatting_mathematics_symbols
\mathcal{A}   # calligraphic / fancy
\mathfrak{B}
\mathbb{C}

# dot above
\dot{x}
\ddot{x}

# spacing
\                # single space
\hspace{1cm}     # 1cm space 
\break
\hfill\break
