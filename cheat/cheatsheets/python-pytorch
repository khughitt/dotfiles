# tensor creation examples
torch.ones(shape)
torch.rand(shape)
torch.zeros(shape)

torch.from_numpy(arr)

# info
tensor.shape
tensor.dtype
tensor.device

# export
tensor.numpy()
tensor.tolist()

# transfer tensor gpu
tensor.cuda()

# transfer tensor to specific gpu
cuda = torch.device('cuda') 
torch.tensor(..., device=cuda)
torch.tensor(...).to(device=cuda)
